
# Supabase credentials
VITE_SUPABASE_URL=https://your-supabase-project-url.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
VITE_AGENT_ENDPOINT=your-agent-api-endpoint
VITE_ENABLE_STREAMING=true

# LangFuse integration (admin dashboard links)
VITE_LANGFUSE_HOST_WITH_PROJECT=http://localhost:3000/project/your-project-id

# User Feedback (Video 8) - Required for thumbs up/down and conversation ratings
# Use the same PUBLIC key as the backend - this is safe for frontend use
VITE_LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key
VITE_LANGFUSE_HOST=https://cloud.langfuse.com

# Optional: Custom rating thresholds (comma-separated AI message counts)
# When to show the conversation rating popup. Default: 5,10,15,20,25,30
VITE_RATING_THRESHOLDS=5,10,15,20,25,30
